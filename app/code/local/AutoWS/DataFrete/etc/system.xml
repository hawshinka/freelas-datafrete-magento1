						</collectPostcode>

						<wsdlUrl translate="label">
							<label>Web Service WSDL URL</label>
							<frontend_type>text</frontend_type>
							<sort_order>5000</sort_order>
							<show_in_default>1</show_in_default>
							<show_in_website>1</show_in_website>
							<show_in_store>1</show_in_store>
							<tooltip>Request this information to suporte@parceirodofrete.com.br</tooltip>
						</wsdlUrl>
						<accessLogin translate="label">
							<label>Access Login</label>
							<frontend_type>text</frontend_type>
							<sort_order>6000</sort_order>
